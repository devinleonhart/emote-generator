{
  "compilerOptions": {
    "baseUrl": "../",
    "paths": {
      "src/types/*": ["src/types/*"],
      "@types/*": ["src/types/*"],
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/stores/*": ["src/stores/*"],
      "@/composables/*": ["src/composables/*"],
      "@/utils/*": ["src/*"],
      "@/util": ["src/util"],
      "@/settings": ["src/settings"]
    },
    "target": "ES2022",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
